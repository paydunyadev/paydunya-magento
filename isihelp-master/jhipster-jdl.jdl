entity Question {
	id Long
	libelle String
    date LocalDate 
    cloturer Boolean 
    
}
entity Commentaire {
	id Long
	libelle String
    date LocalDate 
    vote Integer 
    
}

entity Techno {
	id Long
	libelle String
    publish Boolean 
    
}

entity Langage {
	id Long
	libelle String
    publish Boolean 
    
}


relationship OneToMany {
	Techno to Question{techno},
    Langage to Question{langage},
    Question to Commentaire{question},
}
